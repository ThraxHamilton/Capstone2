@model IEnumerable<Capstone2.Models.Pros>

@{
    ViewData["Title"] = "Index";
}

<h2>Positives</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProEntry)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td id="pro-date">
        @Html.DisplayFor(modelItem => item.Date)
    </td>
    <td id="pro-entry">
        @Html.DisplayFor(modelItem => item.ProEntry)
    </td>
    <td>
        <a asp-action="Edit" asp-route-id="@item.ProId">Edit</a> |
        <a asp-action="Details" asp-route-id="@item.ProId">Details</a> |
        <a asp-action="Delete" asp-route-id="@item.ProId">Delete</a>
    </td>
</tr>
}
    </tbody>
</table>
<li><a asp-area="" asp-controller="Cons" asp-action="Index">Cons</a></li>

